<h1>Course #<%= @course.id %></h1>

<div>
  <strong>Name</strong>:<br />
  <p><%= @course.name %></p>
</div>
<div>Course's Students</div>
<ul>
  <% @course.course_students.each do |course_students| %>
  <li><%= course_students.student.name %></li>
  <% end %>
</ul>
<button><a href="/courses/<%= @course.id %>/edit">Edit</a></button>
<button><a href="/courses/<%= @course.id %>/destroy">Destroy</a></button>
